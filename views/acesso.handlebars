<div class="container">
    <div class="forms-container">
        <div class="signin-signup">
            <form action="#" class="sign-in-form">
                <h2 class="title">Login - {{data.key}}</h2>
                <h2></h2>
                <div class="input-field">
                    <i class="fas fa-user"></i>
                    <input id="l_user" type="text" placeholder="Usuário" />
                </div>
                <div class="input-field">
                    <i class="fas fa-lock"></i>
                    <input id="l_pass" type="password" placeholder="Senha" />
                </div>
                <input id="login" type="button" value="Login" class="btn solid" />
                <div id="notificacao-200" class="notificacao sucess">
                    <i class="fas fa-solid fa-circle-check"></i>
                    <p id=""> Login realizado com sucesso</p>
                </div>
                <div id="notificacao-404" class="notificacao error">
                    <i class="fa-solid fa-circle-exclamation"></i>
                    <p id=""> Falha ao realizar o login</p>
                </div>
            </form>
            <form action="#" class="sign-up-form">
                <h2 class="title">Cadastro</h2>
                <div class="input-field">
                    <i class="fas fa-user"></i>
                    <input id="c_user" type="text" placeholder="Usuário" />
                </div>
                <div class="input-field">
                    <i class="fas fa-envelope"></i>
                    <input id="c_email" type="email" placeholder="E-mail" />
                </div>
                <div class="input-field">
                    <i class="fas fa-lock"></i>
                    <input id="c_pass" type="password" placeholder="Senha" />
                </div>
                <div class="input-field">
                    <i class="fas fa-lock"></i>
                    <input id="c_passc" type="password" placeholder="Confirme a senha" />
                </div>
                <input id="cadastro" type="button" class="btn" value="Cadastrar" />
                <div id="notificacao-401" class="notificacao warn">
                    <i class="fa-solid fa-triangle-exclamation"></i>
                    <p id="">Prencha todos os campos!</p>
                </div>
                <div id="notificacao-402" class="notificacao warn">
                    <i class="fa-solid fa-triangle-exclamation"></i>
                    <p id="">A senha dever conter, no mínimo 8 caracteres!</p>
                </div>
                <div id="notificacao-403" class="notificacao warn">
                    <i class="fa-solid fa-triangle-exclamation"></i>
                    <p id="">As senhas devem coincidir!</p>
                </div>
                <div id="notificacao-201" class="notificacao sucess">
                    <i class="fas fa-solid fa-circle-check"></i>
                    <p id="">Cadastro realizado com sucesso!</p>
                </div>
                <div id="notificacao-405" class="notificacao warn">
                    <i class="fa-solid fa-circle-exclamation"></i>
                    <p id="">Usuário indisponível</p>
                </div>
                <div id="notificacao-406" class="notificacao warn">
                    <i class="fa-solid fa-circle-exclamation"></i>
                    <p id="">E-mail indisponível</p>
                </div>
                <div id="notificacao-407" class="notificacao error">
                    <i class="fa-solid fa-circle-exclamation"></i>
                    <p id="">Falha ao realizar o cadastro</p>
                </div>
            </form>
        </div>
    </div>

    <div class="panels-container">
        <div class="panel left-panel">
            <div class="content">
                <h3>Novo por aqui?</h3>
                <p>
                    Faça o seu cadastro e aproveite as diversas ofertas que te esperam!
                </p>
                <button class="btn transparent" id="sign-up-btn">
                    Cadastrar
                </button>
            </div>
            <img src="/images/luffy.png" class="image" alt="" />
        </div>
        <div class="panel right-panel">
            <div class="content">
                <h3>Já possui uma conta?</h3>
                <p>
                    Faça login e tenha mais liberdade!
                </p>
                <button class="btn transparent" id="sign-in-btn">
                    Login
                </button>
            </div>
            <img src="images/naruto.png" class="image" alt="" />
        </div>
    </div>
</div>